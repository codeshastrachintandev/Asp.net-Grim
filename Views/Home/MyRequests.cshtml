<!-- The Modal -->
    <div class="modal fade timeline-modal-wrap" id="indent-timeline-modal">
        <div class="modal-dialog modal-dialog-centered">
            <div class="modal-content">

                <!-- Modal Header -->
                <div class="modal-header">
                    <h4 class="modal-title">Indent Timeline</h4>
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                </div>

                <!-- Modal body -->
                <div class="modal-body">
                    <div class="timeline" id="timeline">
                
                    </div>
                </div>

                <!-- Modal footer -->
                <div class="modal-footer">
                    <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                </div>

            </div>
        </div>
    </div>
    <!-- The Modal -->
    <div class="modal fade timeline-modal-wrap" id="indentApproval">
        <div class="modal-dialog modal-dialog-centered">
            <div class="modal-content">

                <!-- Modal Header -->
                <div class="modal-header">
                    <h4 class="modal-title">Indent Approval Flow</h4>
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                </div>

                <!-- Modal body -->
                <div class="modal-body" id="modal-body">
                    <div class="timeline" id="timelinebody">

                    </div>
                </div>
            </div>
        </div>
    </div>

    <!-- The Modal for edit -->
    <div class="modal fade timeline-modal-wrap" id="indentApprovaledit">
        <div class="modal-dialog modal-dialog-centered">
            <div class="modal-content">

                <!-- Modal Header -->
                <div class="modal-header">
                    <h4 class="modal-title">Edit Indent Item</h4>
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                </div>

                <!-- Modal body -->
                <div class="modal-body">
                    <div id="timelinebodyedit">
                        <form>
                            <div class="form-group">
                                <div class="row">
                                    <div class="col-6"> 
                                        <label for="recipient-name" class="col-form-label">Item Name:</label>
                                        <input type="text"  disabled="disabled" class="form-control quantity " id="ItemName" value="" required>
                                    </div>
                                    <div class="col-6"> 
                                        <label for="recipient-name" class="col-form-label">Oty:</label>
                                        <input type="number"  min="1" class="form-control quantity quantity" id="iteminput1" value="" required>
                                    </div>
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="">
                                        <select class="form-control DeliveryPriority DeliveryPriority" data-obj="" placeholder="Delivery Priority*" id="iteminput2">
                                            <option value="High">High</option>
                                            <option value="Medium">Medium</option>
                                            <option value="Low">Low</option>
                                        </select>
                                    <span class="error-msg cartdropbox4" id="DeliveryPriority" style="display: none;">*The DeliveryPriority field is required.</span>
                                </div>
                            </div>
                            <div class="form-group">
                                <label for="message-text" class="col-form-label">Priority days*:</label>
                                <input type="text" id="iteminput3" disabled name="input1" class="form-control" required>
                                <p>0 / 3</p>
                                <span class="error-msg cartdropbox4" id="" style="display: none;">*The DeliveryPriority field is required.</span>
                            </div>
                            <div class="form-group">
                                <label for="message-text" class="col-form-label">Tracking No*:</label>
                                <input type="text" class="form-control input1" id="iteminput4" value="test"  required>
                                <p>0/ 3</p>
                                <span class="error-msg cartdropbox4"  style="display: none;">*The DeliveryPriority field is required.</span>
                            </div>
                            <div class="form-group">
                                <label for="message-text" class="col-form-label">Section*:</label>
                                <input type="text" class="form-control input"id="iteminput5" value="test"  required>
                                <p>0 / 20</p>
                                <span class="error-msg cartdropbox4"  style="display: none;">*The DeliveryPriority field is required.</span>
                            </div>
                            <div class="form-group">
                                <label for="message-text" class="col-form-label">Reason*:</label>
                                <textarea class="form-control" id="iteminput6" required></textarea>
                                <p>0 / 100</p>
                                <span class="error-msg cartdropbox4"  style="display: none;">*The DeliveryPriority field is required.</span>
                            </div>
                            <div class="form-group">
                                <label for="message-text" class="col-form-label">Wher Uesd*:</label>
                                <textarea class="form-control" id="iteminput7" required></textarea> 
                                <p>0 / 100</p>
                                <span class="error-msg cartdropbox4" style="display: none;">*The DeliveryPriority field is required.</span>
                            </div>
                        </form>
                    </div>
                </div>
                <div class="modal-footer" id="modal-footer">
                    <button type="button" class="common-button common-red-button" data-dismiss="modal" onclick="popClose('customModalCenter')">cancel</button>
                    <button type="button" class="common-button common-blue-button" id="update">Save</button>
                </div>
            </div>
        </div>
    </div>

